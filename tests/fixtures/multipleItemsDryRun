					<script language="javascript" type="text/javascript">

						function toggleExpandedText(tabId){
							if(document.getElementById('ExpandedTextContent'+tabId).style.display=='block'){
								hideExpandedText(tabId);
							} else {
								showExpandedText(tabId);
							} 
						}

						function showExpandedText(tabId){
							document.getElementById('ExpandedTextContent'+tabId).style.display='block';
							document.getElementById('hideExpandedTextLink'+tabId).style.display=moreOrLessLinkDisplayStyle(tabId);
							document.getElementById('showExpandedTextLink'+tabId).style.display='none';
						}

						function hideExpandedText(tabId){	  
							document.getElementById('ExpandedTextContent'+tabId).style.display='none';
							document.getElementById('hideExpandedTextLink'+tabId).style.display='none';
							document.getElementById('showExpandedTextLink'+tabId).style.display=moreOrLessLinkDisplayStyle(tabId);
							document.getElementById('menuplusnav').focus();	
						}
												
						function moreOrLessLinkDisplayStyle(tabId){
							var blockOrNone = 'block';
							if(document.getElementById('ExpandedTextContent'+tabId).innerHTML.replace(/^\s+|\s+$/g,"").length==0){
								blockOrNone = 'none';
							}
							return blockOrNone;
						}
						
						
						function changeMenuPlusTab(id){
						    var theForm = document.getElementById("menuPlusForm");
						    var idsElement = document.getElementById("menutopiaMenuPlusTabIds");
						    var ids = idsElement.value.split("|");
						    for(var i=0; i<ids.length; i++) {
							if (ids[i] == id) {
							    var tab = document.getElementById("MenuPlusTabSpan" + ids[i]);
							    tab.style.display = "block";							    							
							    var link = document.getElementById("MenuPlusTabLink" + ids[i]);
							    link.style.display = "none";
							} else {							
							    var tab = document.getElementById("MenuPlusTabSpan" + ids[i]);
							    tab.style.display = "none";							    							
							    var link = document.getElementById("MenuPlusTabLink" + ids[i]);
							    link.style.display = "block";
							}
						    }
						    
						    document.getElementById("MenuPlusContentTab"+ activeMenuPlusTabId).style.display='none';
						    document.getElementById("MenuPlusContentTab"+ id).style.display='block';
						    activeMenuPlusTabId = id;
						    
						    var IsConsumer = document.getElementById("IsConsumer").value;						    
						    if(IsConsumer == 'True') {
							setCookie("activeMenuPlusTabId", activeMenuPlusTabId);
						    }
						}
						function setCookie(name, value){ 
							document.cookie = name + "=" + escape(value); 
						}						
						var activeMenuPlusTabId = 25;

					</script>

