        <script type="text/javascript">
            window.addEvent('domready', function () {
                var NumberOfLinesForDisplay = '20'; 
		smartHeight();
		updatePriceList();
		ensureDefaults();
            });

	    function selectOnFocus() {
		document.getElementById('quantity').select();
	    }

            function NotifyCoremetricsAndThenAddItemToOrder() {
                document.getElementById("a1").style.display = 'none';
                document.getElementById("a1loading").style.display = '';
                if (typeof (cmCreateShopAction5Tag) === 'function') {
                    cmCreateShopAction5Tag('8695689', 'Cayenne Shrimp Salad', document.getElementById('quantity').value, '$9.99', null, null, null);
                    cmDisplayShop5s();
                }
		ClickAddToOrder('29', '1486559510');
}

function ensureDefaults () {
    jQuery('.hidden').each(function() {
	var elem = jQuery(this), 
	    nameSplit = elem.attr('name').split('_'), 
	    isMinOptions = nameSplit[0], 
	    groupName = nameSplit[1];

	if (isMinOptions === "minOptions" && elem.attr('value') === "1") { 
	    if (jQuery('input[name="abc' + groupName + '"]:checked').length === 0) { 
		jQuery('input[name="abc' + groupName + '"]:first').attr('checked', true);
	    }
	}
    });
}

var updatePriceLastPrice = '9.9900';

function updatePriceList() {  
    var priceControlArray = [], 
	priceControlArrayMax = [], 
	priceControlArrayCount = [], 
	extras = 0, 
	newPrice = 0, 
	priceControlArrayIndex = 0, 
	originalPrice = '9.9900';

    var all = $(document.body).getElementById('popup').getElements('.checkbox');
    for (var allLoop=0; allLoop<all.length; allLoop++) {
	var current = all[allLoop], 
	    thisIncludedMax = current.getAttribute("price").toString().split("_")[1], 
	    thisGroupName = current.name.toString().split('_')[0], 
	    thisPrice = current.getAttribute("price").toString().split("_")[0];
	if (current.checked) {
	    if (thisIncludedMax !== '0') { 
		var indexofArray = -1; 
		for (var includedLoop=0; includedLoop < priceControlArray.length; includedLoop++) {
		    if (priceControlArray[includedLoop] == thisGroupName) {
			indexofArray = includedLoop; 
			break;
		    }
		}
		if (indexofArray === -1) { 
		    priceControlArray[priceControlArrayIndex] = thisGroupName;
		    priceControlArrayMax[priceControlArrayIndex] = thisIncludedMax;
		    priceControlArrayCount[priceControlArrayIndex] = 1; 
		    priceControlArrayIndex = parseInt(priceControlArrayIndex) + 1;
		} else {
		    if (parseInt(priceControlArrayMax[indexofArray]) <= parseInt(priceControlArrayCount[indexofArray])) {
			extras = parseFloat(extras) + parseFloat(thisPrice);
			priceControlArrayCount[indexofArray] = parseInt(priceControlArrayCount[indexofArray]) + 1;
		    } else {
			priceControlArrayCount[indexofArray] = parseInt(priceControlArrayCount[indexofArray]) + 1;
		    }
		}
	    } else {
		extras = parseFloat(extras) + parseFloat(thisPrice);
	    }
	}
    }

    newPrice = (parseFloat(originalPrice) + parseFloat(extras)) * parseFloat(document.getElementById('quantity').value);

     
    var currencySymbol = "$"; 

    if (!isNaN(document.getElementById('quantity').value)) {
	if (parseFloat(newPrice) !== updatePriceLastPrice) {
	    if (isNaN(parseInt(newPrice)) || parseInt(newPrice) <= 0) {
		document.getElementById("bprice").innerHTML = '--';  
	    } else { 
		document.getElementById("bprice").innerHTML = currencySymbol + newPrice.toFixed(2).toString();  
	      }
	}
    
	updatePriceLastPrice = newPrice;
    } else { 
	var cleanQuantity = document.getElementById('quantity').value.replace(/[^0-9]/g, '');
	document.getElementById('quantity').value = cleanQuantity;
    }    
}

function smartHeight() {  
    var isiPad = navigator.userAgent.match(/iPad/i) !== null;
     
    if (!isiPad) {
	var maxwindowheight = window.getHeight(), 
	    boxframe = 250, 
	    boxbuffer = 100, 
	    availableheight = maxwindowheight - boxframe - boxbuffer, 
	    eachrow = 36, 
	    numberoflines = '20', 
	    neededheight = (numberoflines * eachrow) + (2 * eachrow); // 2* eachrow includes 0 rows and menu feedback link

	if (availableheight < neededheight) {
	    document.getElementById("TB_ajaxContent").style.height = (window.getHeight() - boxbuffer) + 'px';
	    document.getElementById("itemoptions").className = 'scroll';
	}
	else { 
	    document.getElementById("TB_ajaxContent").style.height = (eachrow * numberoflines) + (2 * eachrow) + boxframe + 'px';
	}
    }
}

</script>
