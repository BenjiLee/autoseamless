<script type="text/javascript" language="javascript"> 
<!--
    // isEmail (STRING s [, BOOLEAN emptyOK])
    // whitespace characters
    var whitespace = " \t\n\r";
    function isValidEmail(s) {
        if (isEmpty(s)) return false;

        if (s == 'Email Address') return false;

        // is s whitespace?
        if (isWhitespace(s)) return false;

        // there must be >= 1 character before @, so we
        // start looking at character position 1 
        // (i.e. second character)
        var i = 1;
        var sLength = s.length;

        // look for @
        while ((i < sLength) && (s.charAt(i) != "@")) {
            i++
        }

        if ((i >= sLength) || (s.charAt(i) != "@")) return false;
        else i += 2;

        // look for .
        while ((i < sLength) && (s.charAt(i) != ".")) {
            i++
        }

        // there must be at least one character after the .
        if ((i >= sLength - 1) || (s.charAt(i) != ".")) return false;
        else return true;
    }

    // Check whether string s is empty.
    function isEmpty(s) {
        return ((s == null) || (s.length == 0))
    }

    // Returns true if string s is empty or 
    // whitespace characters only.
    function isWhitespace(s) {
        var i;

        // Is s empty?
        if (isEmpty(s)) return true;

        // Search through string's characters one by one
        // until we find a non-whitespace character.
        // When we do, return false; if we don't, return true.
        for (i = 0; i < s.length; i++) {
            // Check that current character isn't whitespace.
            var c = s.charAt(i);

            if (whitespace.indexOf(c) == -1) return false;
        }
        // All characters are whitespace.
        return true;
    }

    function isValidZip(s) {
        if (s == 'Zip Code' || s == '') return false;
        return true;
    }
    function checkForm() {
        var subscribeForm = document.forms['subscribeForm'];
        var errormsg = '';

        if (document.getElementById("Email_error").style.display != 'none' || document.getElementById("Email_formaterror").style.display != 'none' ) {
            subscribeForm.elements['Email'].style.cssText = 'border-color: #f00 !important; -moz-box-shadow: 0 0 5px rgba(204,0,0,0.5) !important; -webkit-box-shadow: 0 0 5px rgba(204,0,0,0.5) !important;';
            errormsg += '   Please enter a valid email address.';
        }

        var zipCodeInput = subscribeForm.elements['ZipCode'];

        if (!isValidZip(zipCodeInput.value) || document.getElementById("ZipCode_error").style.display != 'none') {
            zipCodeInput.style.cssText = 'border-color: #f00 !important; -moz-box-shadow: 0 0 5px rgba(204,0,0,0.5) !important; -webkit-box-shadow: 0 0 5px rgba(204,0,0,0.5) !important;';
            errormsg += '   Please enter a zip code.';
        }

        if (errormsg != '') {
            return alert(errormsg);
        } else {
            var progressOverlay = new Element('div', { 'class': 'progressOverlay' });
            progressOverlay.inject(subscribeForm.parentNode);

            SubmitFormWithAjax(subscribeForm, function () {
                subscribeForm.style.display = "none";
                progressOverlay.dispose();
                $$(".success")[0].style.display = "block";
            });
        }
    }
    function CheckSubscribed() {
        var subscribed = getQueryString("subscribed", window.location.href);
        if (subscribed) {
            document.getElementById('substhx').style.display = "block";
            document.getElementById('subs').style.display = "none";
        }
    }
    function getQueryString(variable, URL) {
        var query = URL;
        var vars = query.split("?");
        for (var i = 0; i < vars.length; i++) {
            var pair = vars[i].split("=");
            if (pair[0] == variable) {
                return pair[1];
            }
        }
    }

    function currentDate() {
        var currentDate = new Date();
        var dd = currentDate.getDate();
        var mm = currentDate.getMonth() + 1;
        var yyyy = currentDate.getFullYear();
        document.getElementById('field_29').value = mm + '/' + dd + '/' + yyyy;
    }
