    function DivideCost(intAllocCount,OrderTotal) {
        var curField;
        var intDivisions;
        document.pageForm.totalAllocated.value = 0.00;
        intDivisions = intAllocCount - 1;
        if( document.pageForm.NewAllocationAmt != null )
            additionalDivisions = document.getElementById("AddUser").style.display == "table-row" ? 2 : 1
        for (i=1;i<=intDivisions;i++){
            document.getElementById("AllocationAmt" + i).value = FixCurrency((OrderTotal / (intDivisions + additionalDivisions)));
            document.pageForm.totalAllocated.value = parseFloat(document.pageForm.totalAllocated.value) + parseFloat(document.getElementById("AllocationAmt" + i).value)
        }
        curField = "AllocationAmt" + i;
        if( additionalDivisions == 2 ){
            document.pageForm.NewAllocationAmt.value = FixCurrency((OrderTotal / (intDivisions + additionalDivisions)));
            document.pageForm.totalAllocated.value = parseFloat(document.pageForm.totalAllocated.value) + parseFloat(document.pageForm.NewAllocationAmt.value)
        }
        document.pageForm[curField].value = 0;
        document.pageForm[curField].value = FixCurrency((OrderTotal - document.pageForm.totalAllocated.value));
        document.pageForm.totalAllocated.value = "$" + FixCurrency(parseFloat(document.pageForm.totalAllocated.value) + parseFloat(document.getElementById(curField).value));
        SumAllocs(intAllocCount); 
    }

    function SumAllocs(intAllocCount) {
        var curField;

        document.pageForm.TotalAlloc.value = 0.00;
        for (i=1;i<=intAllocCount;i++) {
            curField = "AllocationAmt" + i.toString();
            document.pageForm[curField].value = FixCurrency(stripNonNumeric(document.pageForm[curField].value));
            document.pageForm.TotalAlloc.value = parseFloat(document.pageForm.TotalAlloc.value) + parseFloat(stripNonNumeric(document.pageForm[curField].value));
        }
	    
        if (document.pageForm.NewAllocationAmt != null && document.pageForm.NewAllocationAmt.value != ""){
            if (document.getElementById("AddUser").style.display == "table-row" ) {
                document.pageForm.TotalAlloc.value = parseFloat(stripNonNumeric(document.pageForm.TotalAlloc.value)) + parseFloat(stripNonNumeric(document.pageForm.NewAllocationAmt.value));
                document.pageForm.TotalAlloc.value = FixCurrency(stripNonNumeric(document.pageForm.TotalAlloc.value));
            }
        }

        document.pageForm.totalAllocated.value = currencyUnit + FixCurrency(document.pageForm.TotalAlloc.value);
        var nonAllocatedSum = grandTotalAmount - document.pageForm.TotalAlloc.value;
        var overageAllocationUpdated = 0;
        var remainingToBeAllocatedUpdated = 0;
        if(hasOverageAllocation && nonAllocatedSum >= 0) {
            overageAllocationUpdated = nonAllocatedSum;            
        } else {
            remainingToBeAllocatedUpdated = nonAllocatedSum;
        }     
        if(hasOverageAllocation) {
            document.pageForm.OverageAllocationAmt.value = FixCurrency(overageAllocationUpdated);
            document.getElementById('OverageAllocation').innerHTML = FormatCurrencyBySign(overageAllocationUpdated, currencyUnit);            
        }   
        document.getElementById('RemainingToBeAllocated').innerHTML = FormatCurrencyBySign(remainingToBeAllocatedUpdated, currencyUnit);
    	    
    }

    function SetUserSearch(SearchType) {
        document.pageForm.UserSearchType.value = SearchType;
        RemoveAllocationSearchNameValues();
        doPost(document.pageForm, 'UpdateMealsAllocation.m')
    }
	

    function UserSearch(SearchType) {	    
        var isLoaded = false;
        var expenseCode = "";
        if (document.pageForm.NewExpenseCode != null) {
            expenseCode = document.pageForm.NewExpenseCode.value;
        }

        var expenseCode1 = "";
        var expenseCode1Control = document.getElementById("ExpenseCode1");
        if (expenseCode1Control != null) {
            if (expenseCode1Control != null && expenseCode1Control.value.toString().length > 0) {
                expenseCode1 = document.getElementById("ExpenseCode1").value;
            }
        }

        switch (SearchType) {
        case "Allocation" :
            UserSearchWindow = window.open("GetUsers.m?firmId=11082&orderId=1486563269&userId=4000000&firstName=" + document.pageForm.FirstName.value + "&lastName=" + document.pageForm.LastName.value + "&showAddUser=" + document.pageForm.ShowAddUser.value + "&newAllocationAmt=" + document.pageForm.NewAllocationAmt.value + "&newExpenseCode=" + expenseCode + "&ExpenseCode1=" + expenseCode1, "UserSearch","menubar=yes,toolbar=no,width=715,height=400,location=no,directories=no,status=no,menubar=no,scrollbars=yes,resizable=no");
            break;
        case "ApprovedBy" : 
            UserSearchWindow = window.open("UserNameVerify.asp?FirstName=" + escape(document.pageForm.ApprovedFirstName.value) + "&LastName=" + escape(document.pageForm.ApprovedLastName.value) + "&FirmID=11082&SearchType=" + SearchType, "UserSearch","menubar=yes,toolbar=no,width=610,height=220,location=no,directories=no,status=yes,menubar=no,scrollbars=yes,resizable=no");
            break;
        case "AllocatedUsers" :
            UserSearchWindow = window.open("GetUsers.m?firmId=11082&orderId=1486563269&userId=4000000&firstName=" + document.pageForm.FirstName.value + "&lastName=" + document.pageForm.LastName.value + "&newAllocationAmt=" + document.pageForm.NewAllocationAmt.value + "&newExpenseCode=" + expenseCode, "AllocatedUser","menubar=yes,toolbar=no,width=510,height=400,location=no,directories=no,status=no,menubar=no,scrollbars=yes,resizable=no");			
            break;
        case "RecentAllocations" :
            UserSearchWindow = window.open("GetRecentAllocations.m?firmId=11082&orderId=1486563269&userId=4000000", "UserSearch","menubar=yes,toolbar=no,width=715,height=390,location=no,directories=no,status=no,menubar=no,scrollbars=yes,resizable=no"); 
            break;
        case "Terms" :
            UserSearchWindow = window.open("./business/conditions.html", "UserSearch","menubar=yes,toolbar=no,width=610,height=520,location=no,directories=no,status=yes,menubar=no,scrollbars=yes,resizable=no");
            break;
        case "AboutUs" :
            UserSearchWindow = window.open("./business/about.html", "UserSearch","menubar=yes,toolbar=no,width=610,height=520,location=no,directories=no,status=yes,menubar=no,scrollbars=yes,resizable=no");
            break;
        case "Contact" : 
            UserSearchWindow = window.open("./contact-us", "UserSearch","menubar=yes,toolbar=no,width=610,height=520,location=no,directories=no,status=yes,menubar=no,scrollbars=yes,resizable=no");
            break;
        case "TellaColleague" :
            UserSearchWindow = window.open("TellAColleague.m", "UserSearch","menubar=yes,toolbar=no,width=910,height=620,location=no,directories=no,status=yes,menubar=no,scrollbars=yes,resizable=no");
            break;
        case "News" :
            UserSearchWindow = window.open("http://seamlessweb-news.tumblr.com/main", "UserSearch","menubar=yes,toolbar=no,width=610,height=520,location=no,directories=no,status=yes,menubar=no,scrollbars=yes,resizable=no");
