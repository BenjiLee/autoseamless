    <script type="text/javascript">
        jQuery.noConflict();

        jQuery(function () {
            if (jQuery('#sortBy').length != 0) {
                var sortBy = jQuery('#sortBy').val();
                var sortDirection = jQuery('#sortDirection').val();
                jQuery('.sortNameLink').unbind('click');
                if (sortBy.length < 1) {
                    jQuery('#sortName').show();
                    jQuery('#sortName').attr('src', 'img/corporate/carrow-up.png');
                    jQuery('.sortNameLink').bind('click', function() {
                        document.getElementById('sortBy').value = "Name";
                        document.getElementById('sortDirection').value = "Normal";
                    });
                } else if (sortDirection == 'Normal') {
                    jQuery('#sort' + sortBy).show();
                    jQuery('#sort' + sortBy).attr('src', 'img/corporate/carrow-up.png');
                } else {
                    jQuery('#sort' + sortBy).attr('src', 'img/corporate/carrow-down.png');
                    jQuery('#sort' + sortBy).show();
                }
            }
        }); 


    </script>
