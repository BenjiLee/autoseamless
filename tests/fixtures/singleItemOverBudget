            break;
	    
        }	    
	
    }
	

    function toggleAddUser(show,showAgain) {
        var obj = document.getElementById("AddUser");
        var obj1 = document.getElementById("AddUser1");
        var obj2 = document.getElementById("FirstName");
        var allocs = document.getElementById('allocCount');

        if (show) {
            obj.style.cssText = "display:table-row;visibility:visible;";
            obj.className = obj.className.replace(/\s*Hidden/, "");
            if(allocs > 1)
            {
                obj1.style.cssText = "display:block;visibility:visible;";
            }
            
					
            if(document.pageForm.FirstName.value.length > 0 || document.pageForm.LastName.value.length > 0){
                UserSearch('Allocation');		    
            }
        } else {
            obj.style.cssText = "display:none;visibility:hidden;";
            obj.className += " Hidden";
            RemoveAllocationSearchNameValues();
        }
        if(showAgain){
            document.pageForm.ShowAddUser.value = "Y";
        }else{
            document.pageForm.ShowAddUser.value = "N";
        }
    }
	
    function ConfirmRemoveAllItems() {
        if (confirm("Are you sure that you want to remove all items from this order?"))
        {
            location.href='RemoveAllItemsFromOrder.m?postAction=Meals'
        }
    }		
	
    window.addEvent('domready', function() {
        var is_chrome = navigator.userAgent.toLowerCase().indexOf('chrome') > -1;
        if (is_chrome) {
            var editCreditCardInfo = document.getElementById('EditCreditCardInfo');
            if(editCreditCardInfo != null) {
                var creditCardHtml = editCreditCardInfo.innerHTML;
                editCreditCardInfo.innerHTML = creditCardHtml;
            }
        }
        
        
    });
    	
