    function doPost(form, url) {
        form.action = url;
        if (document.messageForm.AcceptedWarnings != null) {
            form.AcceptedWarnings.value = document.messageForm.AcceptedWarnings.value;
        }
        form.submit();
    }

    function ConfirmDelete(strDestURL, lineId, strProductName) {
        if (confirm("Are you sure that you want to delete '" + strProductName + "'?")) {
            writeLine(lineId);
            doPost(document.pageForm, strDestURL);
        }
    }

    function ConfirmRemoveAllItems() {
        if (confirm("Are you sure that you want to remove all items from this order?")) {
