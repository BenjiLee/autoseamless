                    for (var y = 0; y < myArray.length; y++) {
                        if (document.forms.orderAttributes.elements[myArray[y]].length != null) {
                            for (var i = 0; i < document.forms.orderAttributes.elements[myArray[y]].length; i++) {
                                document.orderAttributes.elements[myArray[y]][i].checked = false;
                                document.orderAttributes.elements[myArray[y]][i].disabled = true;
                            }
                        } else {
                            document.orderAttributes.elements[myArray[y]].checked = false;
