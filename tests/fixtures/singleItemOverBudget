<script>
function ShowConfirmationDialogsIfAny(){
var action;
var form;
if (document.pageForm) {
form = document.pageForm;
action = document.pageForm.saveFavoriteCommand.value
} else {
form = document.forms(0);
action = document.forms(0).action;
}if (confirm("\nOtto Lunch has exceeded the meal allowance designated by your firm. You may proceed, but you will be asked to put the amount in excess of your budget on a personal credit card (see credit card boxes at the bottom of the page).\n") == true){
var el = document.getElementById("AcceptedWarnings");
if(el == null) {var el = document.createElement("input");
el.type = "hidden";
el.name = "AcceptedWarnings";
form.appendChild(el);
}el.value = "Y";
doPost (form, action);
} else {
var el = document.createElement("input");
el.type = "hidden";
el.name = "CancelFavorite";
el.value = "Y";
form.appendChild(el);
doPost (form, action);}}
</script>


