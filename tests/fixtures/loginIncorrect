        function doPost(form, url) {
            form.action = url;
            form.submit();
        }
        function EngageLogin(e) {
            var keycode;

            if (window.event) keycode = window.event.keyCode;
            else if (e) keycode = e.which;

            if (keycode == 13) {
                doPost(document.badloginForm, 'https://www.seamless.com/SubmitLogin.m');
            }
        }
