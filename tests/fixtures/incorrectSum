<script type="text/javascript">

    $(document).ready(function () {

        //store saved credit card number if any
        $("#savedCCNumber").val($("#creditCardNumber").val());
        $("#savedCCType").val($("#typeOfCreditCard option:selected").val());

        $("#typeOfCreditCard").change(function () {

            var ccType = $(this).val();
            setCreditCardLogo(ccType, ".cardImage");
            $("#creditCardNumber").val('');
        }
    );
        $("#creditCardNumber").keyup(function () {

            $("#creditCardNumber").removeClass('redBox');
            
            var ccNumber = $(this).val();

            //record edit operation
            $("#ccTextChange").val('yes');

            if (ccNumber == '') {
                setCreditCardLogo("CC", ".cardImage"); //set default image
                $('#typeOfCreditCard').get(0).selectedIndex = 0;
            }
            else {
                var type = getCreditCardType(ccNumber);

                //check if value exists in dropdown , drop down contains values that are supported in culture
                //drop downs are already populated from server side.
                var exists = 0 != $('#typeOfCreditCard option[value=' + type + ']').length;
                if (exists) {
                    $('#typeOfCreditCard').val(type);
                    setCreditCardLogo(type, ".cardImage");
                    $("#currentType").val(type);
                }
                else {
                    setCreditCardLogo("UNKNOWN", ".cardImage");
                    $("#currentType").val("UNKNOWN");
                    $('#typeOfCreditCard').get(0).selectedIndex = 0;
                }
            }
        });
        $("#creditCardNumber").click(function () {

            $(this).val('');
            
            //record edit operation
            $("#ccTextChange").val('yes');
            $("#creditCardNumber").removeClass('redBox');
            var ccNumber = $(this).val();

            //record click inside text box 
            $("#ccClicked").val('');

            if (ccNumber == '') {
                setCreditCardLogo("CC", ".cardImage"); //set default image
                $('#typeOfCreditCard').get(0).selectedIndex = 0;
            }
        });
        $("#creditCardNumber").blur(function () {
            //user clicked but did not changed any text
            if ($("#ccTextChange").val() == 'no' && $("#ccClicked").val() == 'yes') {
                $("#creditCardNumber").val($("#savedCCNumber").val());
                $('#typeOfCreditCard').val($("#savedCCType").val());

            }
        });
        $("#creditCardNumber").focusout(function () {
            //user clicked but did not changed any text
            if ($("#ccTextChange").val() == 'no' && $("#ccClicked").val() == 'yes') {
                $("#creditCardNumber").val($("#savedCCNumber").val());
                $('#typeOfCreditCard').val($("#savedCCType").val());

            }
        });
        var ccType = $("#typeOfCreditCard option:selected").val();
        setCreditCardLogo(ccType, ".cardImage");
    });
</script>
