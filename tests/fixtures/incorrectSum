            var Queue = null;
            function GetQueue(){
                if(Queue == null) {
                    Queue = new net.CommandQueue('Ajax.m');
                }
            }    
  
            function CreateCommand(id, elementId){
                switch(id){
                  
                      case '18':
                        var command = new Ajax.Commands.GetUserTags(elementId,id,ajaxCounter);
                        net.Base.addCommand(command);
                        break;
                  
                      case '17':
                        var command = new Ajax.Commands.AddUserTags(elementId,id,ajaxCounter);
                        net.Base.addCommand(command);
                        break;
                  
                      case '29':
                        var command = new Ajax.Commands.AddToOrder(elementId,id,ajaxCounter);
                        net.Base.addCommand(command);
                        break;
                  
                      case '35':
                        var command = new Ajax.Commands.AddDailySpecialDistributionCorporate(elementId,id,ajaxCounter);
                        net.Base.addCommand(command);
                        break;
                  
                      case '36':
                        var command = new Ajax.Commands.RemoveDailySpecialDistributionCorporate(elementId,id,ajaxCounter);
                        net.Base.addCommand(command);
                        break;
                  
                      case '74':
                        var command = new Ajax.Commands.CorporateOrderSummary(elementId,id,ajaxCounter);
                        net.Base.addCommand(command);
                        break;
                  
                      case '81':
                        var command = new Ajax.Commands.UpdateRateReviewCorp(elementId,id,ajaxCounter);
                        net.Base.addCommand(command);
                        break;
                  
                      case '82':
                        var command = new Ajax.Commands.TagUnTagReviewCorp(elementId,id,ajaxCounter);
                        net.Base.addCommand(command);
                        break;
                  
                }
                IncCounter();
             }

                             

             var ajaxCounter = 0;
             function IncCounter(){
                 ajaxCounter++;
             }           

