            var Queue = null;
            function GetQueue(){
                if(Queue == null) {
                    Queue = new net.CommandQueue('Ajax.m');
                }
            }    
  
            function CreateCommand(id, elementId){
                switch(id){
                  
                      case '38':
                        var command = new Ajax.Commands.EcoToGo(elementId,id,ajaxCounter);
                        net.Base.addCommand(command);
                        break;
                  
                      case '81':
                        var command = new Ajax.Commands.UpdateRateReviewCorp(elementId,id,ajaxCounter);
                        net.Base.addCommand(command);
                        break;
                  
                }
                IncCounter();
             }

                             

             var ajaxCounter = 0;
             function IncCounter(){
                 ajaxCounter++;
             }           

          </script>
