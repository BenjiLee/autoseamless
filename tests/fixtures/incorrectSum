        function GetOrder(orderID, productTotalAmt, cssClass) {
            if (cssClass != 'OrderStep4' && productTotalAmt > 0) {
                var params = "Get|" + orderID + "|" + cssClass;
                try {
                    CreateCommand('29', params);
                } catch (Exception) { }
            }
        }

